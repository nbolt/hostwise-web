#discount-code.applied::after
  color: teal
  content: '\e81a'
  position: absolute
  top: 9px
  right: 9px
  font-family: "hw-icons"
  font-style: normal
  font-weight: normal
  speak: none
  display: inline-block
  text-decoration: inherit
  width: 1em
  text-align: center
  font-variant: normal
  text-transform: none
  line-height: 1em

.ngdialog
  .ngdialog-content
    background: white
    color: darker-gray
    margin: auto
    max-width: 100%
    position: absolute
    top: 50%
    left: 50%
    transform: translate(-50%, -50%)
    -webkit-transform: translate(-50%, -50%)
    -moz-transform: translate(-50%, -50%)
    -ms-transform: translate(-50%, -50%)
    @media phone
      top: 0
      left: 0
      transform: translate(0, 0)
      -webkit-transform: translate(0, 0)
      -moz-transform: translate(0, 0)
      -ms-transform: translate(0, 0)
    .modal
      position: relative
      .flash
        border-radius: 0
        width: 100%
      > .content > .title
        font-size: 1.25em
        margin-bottom: 25px
  .ngdialog-close, i.close
    color: black
    font-size: 1.5em
    margin-right: 6px
    position: absolute
    right: 5px
    top: 9px
    opacity: 0.3
    transition: opacity .25s ease-in-out
    &:hover
      opacity: 0.5
      cursor: pointer
    &::before
      font-family: 'hw-icons'
      content: '\e810'

.ngdialog.auth
