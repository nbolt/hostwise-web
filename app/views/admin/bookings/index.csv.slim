- headers = ['TimeStamp', 'Toiletries', 'Email', 'Linens', 'userID', 'propertyID', 'AppointmentID', 'date', 'name', 'phone', 'Nickname', 'street', 'unit', 'city', 'zip', 'home_type', 'bed', 'bath', 'king', 'queen', 'full', 'twin', 'Staging Fee', 'Pool Area', 'Balcony/Patio', 'Exterior Windows', 'enter', 'trash', 'instructions', 'parking', 'restocking', 'payment']

= CSV.generate_line headers
- @bookings.each do |booking|
  = CSV.generate_line([Time.now.strftime('%I:%M%p %m/%d/%Y'), booking.services.where(name:'toiletries')[0] && 'yes' || 'no', booking.property.user.email, booking.services.where(name:'linens')[0] && 'yes' || 'no', booking.property.user.id, booking.property.id, job.id, booking.date.strftime, booking.property.user.name, booking.property.user.phone_number, booking.property.nickname, booking.property.address1, booking.property.address2, booking.property.city, booking.property.zip, booking.property.property_type_cd == 0 && 'house' || 'condo', booking.property.bedrooms, booking.property.bathrooms, booking.property.king_beds, booking.property.queen_beds, booking.property.full_beds, booking.property.twin_beds, booking.services.where(name:'preset')[0] && '$' + PRICING['preset'][booking.property.beds].to_s || '0', booking.services.where(name:'pool')[0] && '$' + PRICING['pool'].to_s || '0', booking.services.where(name:'patio')[0] && '$' + PRICING['patio'].to_s || '0', booking.services.where(name:'windows')[0] && '$' + PRICING['windows'].to_s || '0', booking.property.access_info, booking.property.trash_disposal, booking.property.additional_info, booking.property.parking_info, booking.property.restocking_info, booking.payment.display]).html_safe
