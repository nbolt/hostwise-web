- headers = ['Booking ID', 'Booking Status', 'Net Sale', 'Adjusted Cost', 'Service Cost', 'Linen Cost', 'Toiletries Cost', 'Late Next Day Cost', 'Late Same Day Cost', 'No Access Fee Cost', 'Extra King Sets Cost', 'Extra Twin Sets Cost', 'Extra Toiletries Cost', 'First Booking Discount Cost', 'Coupon Cost', 'Payment Status', 'Email', 'userID', 'propertyID', 'jobID', 'date', 'name', 'phone', 'Nickname', 'street', 'unit', 'city', 'zip', 'home_type', 'bed', 'bath', 'king', 'queen', 'full', 'twin', 'enter', 'trash', 'instructions', 'parking', 'restocking', 'payment']

= CSV.generate_line headers
- @bookings.each do |booking|
  = CSV.generate_line([booking.id, booking.status, "$#{booking.cost}", booking.adjusted_cost / 100, booking.contractor_service_cost, booking.linen_cost, booking.toiletries_cost, booking.late_next_day_cost, booking.late_same_day_cost, booking.no_access_fee_cost, booking.extra_king_sets_cost, booking.extra_twin_sets_cost, booking.extra_toiletry_sets_cost, "-#{booking.first_booking_discount_cost}", "-#{booking.coupon_dollar_cost}", booking.payment_status_cd == 0 && 'Open' || 'Received', booking.property.user.email, booking.property.user.id, booking.property.id, booking.job.id, booking.date.strftime, booking.property.user.name, booking.property.user.phone_number, booking.property.nickname, booking.property.address1, booking.property.address2, booking.property.city, booking.property.zip, booking.property.property_type_cd == 0 && 'house' || 'condo', booking.property.bedrooms, booking.property.bathrooms, booking.property.king_beds, booking.property.queen_beds, booking.property.full_beds, booking.property.twin_beds, booking.property.access_info, booking.property.trash_disposal, booking.property.additional_info, booking.property.parking_info, booking.property.restocking_info, booking.payment.display]).html_safe
